<script>
    
    function createObject(num) {
        num = +prompt("Введите число от 0 до 999");
        let sap = {
            "units": num,
            "profs": num,
            "hundrds": num
        }
        sap["hundrds"] = num % 10 * 100;
        sap["profs"] = num % 100 % 10 * 10;
        sap["units"] = num % 10;
        console.log (sap);
}
createObject ()


</script>
